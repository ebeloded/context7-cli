{
  "name": "context7-cli",
  "version": "0.1.0",
  "description": "Context7 CLI - Access library documentation from the terminal",
  "type": "module",
  "private": true,
  "bin": {
    "c7": "./src/index.ts"
  },
  "scripts": {
    "start": "bun src/index.ts",
    "build": "bun build --compile --target=bun src/index.ts --outfile c7"
  },
  "dependencies": {
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
